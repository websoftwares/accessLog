{
    "name" : "websoftwares/access-log",
    "description" : "Log PSR-7 http messages using a PSR-3 logger instance following apache2 access log format.",
    "keywords" : [
        "HTTP",
        "Log",
        "Request",
        "PSR7",
        "PSR3"
    ],
    "license" : "MIT",
    "authors" : [
        {
            "name": "Boris Verhaaff",
            "email": "boris@websoftwar.es",
            "role" : "Developer"
        }
    ],

    "require" : {
        "php": ">=5.5.0",
        "psr/http-message": "1.0.0",
        "psr/log": "~1.0.0"

    },
    "autoload": {
        "psr-4": {
            "Websoftwares\\AccessLog\\":  "src/"
        }
    },
    "require-dev": {    
        "phpunit/phpunit": "dev-master",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zend-diactoros": "1.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Websoftwares\\Tests\\AccessLog\\":  "tests/"
        }
    }
}
